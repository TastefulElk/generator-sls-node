{
  "name": "generator-sls-node",
  "version": "1.0.0",
  "description": "Yeoman generator for Serverless node applications",
  "scripts": {
    "test": "jest"
  },
  "keywords": [
    "yeoman-generator",
    "yeoman",
    "yo",
    "sls",
    "serverless",
    "lambda"
  ],
  "author": "Sebastian Pettersson",
  "license": "ISC",
  "dependencies": {
    "yeoman-generator": "^4.12.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "commitizen": "^4.2.2",
    "commitlint": "^11.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "echo 'Running pre-push tests...' && yarn test"
    }
  }
}
